  "sections" : [
    {
      "name": "Prematch",
      "fields": [
        {
          "title": "Scouter Initials",
          "description": "Enter the initials of the scouter.",
          "type": "text",
          "required": true,
          "code": "scouter",
          "formResetBehavior": "preserve",
          "defaultValue": ""
        },
        {
          "title": "Match Number",
          "description": "Enter the match number.",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "formResetBehavior": "increment",
          "defaultValue": 1
        },
        {
          "title": "Robot",
          "description": "The robot you are scouting this match, based on driver station position.",
          "type": "select",
          "required": true,
          "code": "robot",
          "formResetBehavior": "preserve",
          "defaultValue": "R1",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3"
          }
        },
        {
          "title": "Team Number",
          "description": "The team number of the robot you're scouting.",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 19999
        },
        {
          "title": "Starting Position",
          "description": "The starting position of the robot.",
          "type": "select",
          "required": true,
          "code": "Prsp",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "R1": "Not Sure",
            "R2": "Not Sure 2",
            "R3": "Not Sure 3"
          }
        },
        {
          "title": "No Show",
          "description": "Check if the robot did not show up for the match.",
          "type": "boolean",
          "required": false,
          "code": "noShow",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name" : "Autonomous",
      "fields" : [
        {
          "title" : "Have auto?",
          "description" : "Does the robot have an auto?",
          "code" : "A_exists",
          "type" : "boolean"
        }, 
        {
          "title" : "Auto Fuel",
          "description" : "How many fuel did the robot score during auto?",
          "code" : "A_fuel",
          "type" : "counter"
        },
        {
          "title" : "Climb",
          "description" : "Did the robot climp during auto?",
          "code" : "A_climb",
          "type" : "boolean"
        }
      ]
    },
    {
      "name" : "Teleop",
      "fields" : [
        {
          "title" : "Fuel Scored",
          "description" : "The number of points scored for fuel.",
          "code" : "T_fuel",
          "type" : "counter"
        },
        {
          "title" : "Fuel Transferred",
          "description" : "The number of fuel transferred from the center to the alliance area.",
          "code" : "T_fuel_trans",
          "type" : "counter"
        },
        {
          "title" : "Bump Cycles",
          "description" : "The number of times the robot went over the bump.",
          "code" : "T_bump",
          "type" : "counter"
        },
        {
          "title" : "Trench Cycles",
          "description" : "The number of times the robot went through the trench.",
          "code" : "T_trench",
          "type" : "counter"
        }
      ]
    },
    {
      "name" : "Endgame",
      "fields" : [
        {
          "title" : "Climb",
          "description" : "",
          "code" : "E_climb",
          "type" : "select",
          "choices": {
            "L0": "None",
            "L1": "L1",
            "L2": "L2",
            "L3": "L3"
          }
        },
        {
          "title": "Crossed Field/Played Defense?",
          "type": "boolean",
          "required": false,
          "code": "T_defense"
        },
        {
          "title": "Was Robot Defended by Other Alliance?",
          "type": "boolean",
          "code": "T_defended"
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "Offense Skill",
          "type": "range",
          "required": false,
          "code": "P_os",
          "defaultValue": 3,
          "min": 1,
          "max": 5
        },
        {
          "title" : "Shooting Accuracy",
          "description" : "How accurate was the shooter?",
          "code" : "P_accuracy",
          "type" : "select",
          "choices" : {
            "A0" : "Benchy McBenchface",
            "A1" : "Barry Brick",
            "A2" : "Andreas Chrysanthou",
            "A3" : "Sharquille O'Neal",
            "A4" : "LePrawn James"
          }
        },
        {
          "title": "Defensive Skill",
          "type": "range",
          "required": false,
          "code": "iP_ds",
          "defaultValue": 0,
          "min": 1,
          "max": 5
        },
        {
          "title": "Yellow/Red Card",
          "type": "select",
          "required": true,
          "code": "P_card",
          "defaultValue": "No Card",
          "choices": {
            "No Card": "No Card",
            "Yellow": "Yellow Card",
            "Red": "Red Card"
          }
        },
        {
          "title": "Comments",
          "type": "text",
          "required": false,
          "code": "P_comments",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "min": 0,
          "max": 100
        }
      ]
    }
  ]
