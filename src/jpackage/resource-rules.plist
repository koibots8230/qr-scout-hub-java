<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>rules</key>
  <dict>
    <!-- Seal Info.plist -->
    <key>Contents/Info.plist</key>
    <true/>
    <!-- Seal all JAR files -->
    <key>Contents/Java</key>
    <dict>
      <key>.*\.jar$</key>
      <true/>
    </dict>
  </dict>
</dict>
</plist>
